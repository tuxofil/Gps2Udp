<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.tuxofil.android"
          android:versionCode="1"
          android:versionName="1.0">
  <uses-permission android:name="android.permission.INTERNET" />
  <application android:label="@string/app_name"
               android:icon="@drawable/ic_launcher">
    <activity android:name="Gps2UdpActivity"
              android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <service android:name="Gps2UdpService"
             android:exported="false">
      <intent-filter>
        <action android:name="com.tuxofil.android.gps2udp.action.START" />
        <action android:name="com.tuxofil.android.gps2udp.action.STOP" />
      </intent-filter>
    </service>
  </application>
</manifest>
